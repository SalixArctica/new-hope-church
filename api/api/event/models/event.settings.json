{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "events",
  "info": {
    "name": "Event"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "start": {
      "required": true,
      "type": "date"
    },
    "end": {
      "type": "date",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": false
    },
    "picture": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    }
  }
}
